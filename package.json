  "homepage": "https://github.com/w11k/ngx-lifecycle-mixins#readme",
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "del-cli": "3.0.0",
    "jest": "^25.1.0",
    "np": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@angular/core": ">8",
    "rxjs": "6.x.x"
